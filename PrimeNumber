import java.util.*;
public class PrimeNumber {
    static boolean swap(int a){
        int count=0;
        for(int i=1;i<=a;i++){
            if(a%i==0){
                count++;

            }
        }
        return count==2;

                                                         //OWN CODE ----> CopyRights !!

    }




    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        Boolean yes = swap(a);
        if (yes) {
            System.out.println(a + " is a PrimeNumber");
        } else
            System.out.println(a + " Is not a PrimeNumber ");
    }
}
